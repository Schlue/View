{
    "name": "horde/view",
    "description": "View pattern library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_View",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Mike Naberezny",
            "email": "mike@maintainable.com",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-10-08",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/support": "^3 || dev-FRAMEWORK_6_0",
        "horde/util": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "horde/log": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "horde/controller": "^3 || dev-FRAMEWORK_6_0",
        "horde/routes": "^3 || dev-FRAMEWORK_6_0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_View": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\View\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}